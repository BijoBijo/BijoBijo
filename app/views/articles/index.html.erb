<div class="ladiesContentTitle">
  RECENT ARTICLES
</div>
<div class="ladiesContentBorder">
</div>

<div class="row">
  <% @articles.each do |article| %>
    <%= link_to article_path(article) do %>
      <div class="col-md-6 ladyBox" style="background-image: url(<%= article.images.first.name %>);"></div>
    <% end %>
  <% end %>
</div>
